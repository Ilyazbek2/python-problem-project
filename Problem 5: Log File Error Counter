logs = [
    "[ERROR] 2025-01-05 Database failed",
    "[INFO] 2025-01-05 System started",
    "[ERROR] 2025-01-06 Connection lost",
    "[ERROR] 2025-01-06 Timeout error",
]

error_count = 0
errors_by_date = {}

for log in logs:
    if log.startswith("[ERROR]"):
        error_count += 1
        date = log.split()[1]
        errors_by_date[date] = errors_by_date.get(date, 0) + 1

print("Total errors:", error_count)
print("Errors by date:", errors_by_date)
